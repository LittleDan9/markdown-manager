  "description": "Simple Markdown Preview tool with Mermaid Support",
  "private": "true",
  "scripts": {
    "test": "jest",
    "build": "npx webpack --mode=production && npm run copy-prism-themes",
    "copy-prism-themes": "node scripts/copy-prism-themes.js",
    "build:clean": "rm -rf dist && npm run build",
    "build:dev": "npx webpack --mode=development",
    "build:dev:incremental": "npx webpack --mode=development --watch",
    "build:analyze": "npx webpack --mode=production --analyze",
    "debug": "npx webpack serve --mode=development --devtool=source-map",
    "serve": "npx webpack serve --mode=development --watch --devtool=source-map --open",
    "serve:prod": "npx webpack serve --mode=production",
